        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/otherUpdates"
        app:layout_constraintStart_toStartOf="@+id/otherUpdates"
        app:layout_constraintTop_toBottomOf="@+id/otherUpdates"
