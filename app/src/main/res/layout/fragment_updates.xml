                android:fontFamily="sans-serif-medium"
                android:textColor="#362703"
                android:text="1 week or more"
                android:textSize="30sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
