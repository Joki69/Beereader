        android:textColor="#362703"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/historyUpdate"
        app:layout_constraintStart_toStartOf="@+id/historyUpdate"
        app:layout_constraintTop_toBottomOf="@+id/historyUpdate"
