        app:layout_constraintEnd_toEndOf="@+id/browseUpdate"
        app:layout_constraintStart_toStartOf="@+id/browseUpdate"
        app:layout_constraintTop_toBottomOf="@+id/browseUpdate"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Library"
        android:textColor="#362703"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="@+id/libraryUpdate"
        app:layout_constraintStart_toStartOf="@+id/libraryUpdate"
        app:layout_constraintTop_toBottomOf="@+id/libraryUpdate"
        tools:ignore="TextSizeCheck" />

    <ImageView
        android:id="@+id/updateChainComents"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/historyUpdate"
        app:layout_constraintStart_toEndOf="@+id/libraryUpdate"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976"
        app:srcCompat="@drawable/updateblanco" />

    <ImageView
        android:id="@+id/libraryUpdate"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976"
